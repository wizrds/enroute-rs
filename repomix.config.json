{
  "output": {
    "filePath": "repomix.output.md",
    "style": "markdown",
    "instructionFilePath": "repomix.instruction.md",
    "removeComments": false,
    "removeEmptyLines": false,
    "topFilesLength": 5,
    "showLineNumbers": false,
    "copyToClipboard": false
  },
  "include": [
    "enroute/**/*.rs",
    "enroute-macros/**/*.rs",
    "enroute-core/**/*.rs",
    "enroute-memory/**/*.rs",
    "enroute-kafka/**/*.rs",
    "enroute-test/**/*.rs"
  ],
  "ignore": {
    "useGitignore": true,
    "useDefaultPatterns": true,
    "customPatterns": []
  },
  "security": {
    "enableSecurityCheck": true
  }
}